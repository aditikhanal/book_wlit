                                <form action="/sell" method="POST" enctype="multipart/form-data">
                                    User Details:
                                    <div class="row form-group">
                                    
                                        <div class="col-md-6">
                                            <label for="username">Name</label>
                                            <input type="text" id="bname" class="form-control" placeholder="Your Name" name='username'>
                                        </div>
                                        <div class="col-md-6">
                                            <label for="bname">Contact</label>
                                            <input type="text" id="contact" class="form-control" placeholder="Contact" name='contact'>
                                        </div>
                                        <div class="col-md-6">
                                            <label for="bname">Email</label>
                                            <input type="text" id="email" class="form-control" placeholder="Email" name='useremail'>
                                        </div>
                                    </div>
                                    
                                </div>
                                    Book Details
                                    <div class="row form-group">
                                        <div class="col-md-6">
                                            <label for="bname">Book Name</label>
                                            <input type="text" id="bname" class="form-control" placeholder="Book name"
                                                name='name'>
                                        </div>
                                        <div class="col-md-6">
                                            <label for="author">Author</label>
                                            <input type="text" id="author" class="form-control" placeholder="Author"
                                                name='author'>
                                        </div>
                                    </div>


                                    <div class="row form-group">
                                        <div class="col-md-12">
                                            <label for="subject">Genre</label>
                                            <select name='genre'>
                                                <option>Fantasy</option>
                                                <option>Horror</option>
                                                <option>Romance</option>
                                                <option>Tragedy</option>
                                                <option>Action</option>
                                                <option>Adventure</option>
                                                <option>Epic</option>
                                                <option>Comedy-drama</option>
                                                <option>Non-Fiction</option>
                                            </select>
                                        </div>
                                        
                                    </div>
                                    <div class="row form-group">
                                        <div class="col-md-12">
                                            <label for="subject">Condition</label>
                                            <select name='condition'>
                                                <option>new</option>
                                                <option>like new</option>
                                                <option>old but in good condition</option>
                                        </div>
                                    
                                    </div>
                                    <div class="row form-group">
                                        <div class="col-md-12">
                                            <label for="subject">Description</label>
                                            <input type="text" id="Description" class="form-control"
                                                placeholder="Description" name='description'>
                                        </div>
                                    </div>
                               
                                    <div class="col-md-12">
